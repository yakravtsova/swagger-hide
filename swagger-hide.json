                "in": "path",
                "name": "id",
                "required": true,
                "description": "ID задачи в jira-инстансе",
                "schema": {
                  "type": "integer"
                }
              }
            ],
            "responses": {
              "204": {
                "description": "Удаление прошло успешно"
              },
              "403": {
                "description": "Нет доступа"
              },
              "404": {
                "description": "Параметр id некорректный или отсутствует"
              }
            }
          },
          "put": {
